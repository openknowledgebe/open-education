{
  "@context": {
    "education": "https://openknowledgebe.github.io/open-education/voc/#",

    "schema":            "http://schema.org/",
    "Course":            "education:Course",
    "Student":           "education:Student",
    "Group":             "education:Group",
    "Subject":           "education:Subject",
    "Teacher":           "education:Teacher",
    "CourseEvent":       "education:CourseEvent",
    "DiversityGroup":    "education:DiversityGroup",

    "diversityGroup":    "education:diversityGroup",
    "credits":           "education:credits",
    "required":          "education:required",
    "amount":            "education:amount",
    "course":   { "@id": "education:course",  "@type": "@id" },
    "subjects": { "@id": "education:subject", "@type": "@id", "@container": "@list" },
    "courses":  { "@id": "education:course",  "@type": "@id", "@container": "@list" },
    "groups":   { "@id": "education:group",   "@type": "@id", "@container": "@list" },
    "teachers": { "@id": "education:teacher", "@type": "@id", "@container": "@list" },

    "name":              "schema:name",
    "givenName":         "schema:givenName",
    "middleName":        "schema:additionalName",
    "familyName":        "schema:familyName",
    "honorificPrefix":   "schema:honorificPrefix",
    "honorificSuffix":   "schema:honorificSuffix",
    "title":             "schema:title",
    "gender": { "@id":   "schema:gender", "@type": "@id" },
    "startDate":         "schema:startDate",
    "endDate":           "schema:endDate",
    "description":       "schema:description"
  }
}
